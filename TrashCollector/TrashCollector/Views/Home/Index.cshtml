@{
    ViewBag.Title = "Home Page";
}
<style>
    /* Always set the map height explicitly to define the size of the div
        * element that contains the map. */
    #map {
        height: 500px;
        width: 100%
    }
</style>
<br />
<div class="jumbotron">
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 43.038, lng: -87.906 },
                zoom: 13
            });

            $.get("@Url.Action("GetAllLocation", "Home")", function (data, status) {
                var marker = new google.maps.Marker({
                    position: { lat: 43.038, lng: -87.906 },
                    map: map
                });
            })
            
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDF3e3GTWiXubv6-LkNBxmGDUnQzoZlYzQ&callback=initMap"
            async defer></script>
</div>

