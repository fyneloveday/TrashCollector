        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndPickupSuspension)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bill)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
